{
  "schemaVersion": 1,
  "id": "rollback",
  "version": "${version}",
  "name": "Rollback",
  "description": "This mod improves the backup system. (Automatic backups, GUI, etc.)",
  "authors": [
    "Mehrad Nayyeri"
  ],
  "contact": {
    "homepage": "https://github.com/MehradN/Rollback",
    "sources": "https://github.com/MehradN/Rollback",
    "issues": "https://github.com/MehradN/Rollback/issues"
  },
  "license": "GPL-3.0",
  "icon": "assets/rollback/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "ir.mehradn.rollback.Rollback"
    ],
    "client": [
      "ir.mehradn.rollback.RollbackClient"
    ],
    "preLaunch": [
      "ir.mehradn.rollback.RollbackPreLaunch"
    ]
  },
  "mixins": [
    "rollback.mixins.json",
    {
      "config": "rollback.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "java": ">=17",
    "minecraft": "~1.19.4",
    "fabricloader": ">=0.14.19",
    "fabric-api": ">=0.79.0+1.19.4"
  }
}